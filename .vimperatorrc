nmap <BS> <C-b>
nmap q d
nmap <C-j> <Nop>
nmap <S-j> <C-n>
nmap <S-k> <C-p>
nmap <F9> :set gui=none<CR>
nmap <F8> :set gui=tabs<CR>
noremap j 5j
noremap k 5k
set nextpattern+=^\s*下一页\s*$
set previouspattern+=\s*上一页\s*$
map a <Nop>
map A <Nop>
noremap <A-p> iv$y<ESC><ESC>p
noremap <A-S-p> iv$y<ESC><ESC>P
colorscheme my

command! vsplit emenu Tile.Tile New Tab.Right
command! split emenu Tile.Tile New Tab.Below
command test :set gui=tabs

set gui=tabs
nmap M :bmarks<CR>
set hintchars=hjklasdfgyuiopqwertnmzxcvb
set nosb
set complete=lSt
set focuscontent=true
